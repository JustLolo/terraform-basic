p terraform plan
a terraform apply -auto-approve
d terraform destroy -auto-approve
dt terraform destroy -target aws_instance.dev_node[0]
l terraform state list
li terraform state list | grep aws_instance
s terraform state show
v terraform --version
o terraform output
g terraform graph | dot -Tsvg > graph.svg
gd terraform graph -type=plan-destroy | dot -Tsvg > graphDestroy.svg
